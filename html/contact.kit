<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>ForestGroup - CONTACT</title>
  <!-- @include 'style.kit' -->
</head>

<body>

  <!-- @include 'nav.kit' -->
  <!-- @include 'header.kit' -->
  <section id="contact" class="contact">
    <h2 class="section-tittle">Contact</h2>

    <div class="container">
      <div class="contact__box">
        <div class="contact__box-info">
          <div class="contact__box-info-item">
            <i class="fa fa-home"></i>
            <span>
              <h5>Forest Group Ltd.</h5>
              <p>ul. Jasnagóra 2</p>
              <p>31-358 Kraków</p>
            </span>
          </div>
          <div class="contact__box-info-item">
            <i class="fa fa-phone"></i>
            <span>
              <h5>111-222-333</h5>
              <p>From Monday to Firday, 10:00- 18:00</p>
            </span>
          </div>
          <div class="contact__box-info-item">
            <i class="fa fa-envelope"></i>
            <span>
              <h5>forest@office.com</h5>
              <p>Write to us</p>
            </span>
          </div>
        </div>

        <div class="contact__box-info">
          <form action="">
            <input type="text" placeholder="Enter your name" required>
            <input type="email" placeholder="Type your e-mail address" required>
            <input type="text" placeholder="Subject" required>
            <textarea placeholder="Message" cols="8" rows="10" required></textarea>
            <button class="contact__box-info-button" type="submit"> Send message</button>
          </form>

        </div>
      </div>
    </div>

  </section>


  <section class="location">
    <h2 class="location__title  section-tittle">our site</h2>
    <div class="container">
      <div id="map" class="location__map">
        <iframe
          src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2559.898452497729!2d19.890660215591073!3d50.088188079427056!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47165a326d015c3b%3A0x47a12cd367a1f34e!2sMcDonald&#39;s!5e0!3m2!1sen!2suk!4v1658422098421!5m2!1sen!2suk"
          width="650" height="600" style="border:0;" allowfullscreen="" loading="lazy"
          referrerpolicy="no-referrer-when-downgrade"></iframe>
      </div>
    </div>
  </section>
  <!-- @include 'footer.kit' -->

</body>

</html>